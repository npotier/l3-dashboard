<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Camille\PlatformBundle\Repository\Fiche1Repository" name="Camille\PlatformBundle\Entity\Fiche1">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="projectName" type="string" column="projectName" length="255"/>
    <field name="ficheDate" type="datetime" column="ficheDate"/>
    <field name="nbDayDone" type="integer" column="nbDayDone"/>
    <field name="nbDaySold" type="integer" column="nbDaySold"/>
    <field name="progression" type="string" column="progression" length="255"/>
    <field name="comment" type="string" column="comment" length="255"/>
    <field name="creater" type="string" column="creater" length="255"/>
  </entity>
</doctrine-mapping>

