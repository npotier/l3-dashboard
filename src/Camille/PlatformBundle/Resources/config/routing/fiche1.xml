<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="fiche1_index" path="/" methods="GET">
        <default key="_controller">CamillePlatformBundle:Fiche1:index</default>
    </route>

    <route id="fiche1_show" path="/{id}/show" methods="GET">
        <default key="_controller">CamillePlatformBundle:Fiche1:show</default>
    </route>

    <route id="fiche1_new" path="/new" methods="GET|POST">
        <default key="_controller">CamillePlatformBundle:Fiche1:new</default>
    </route>

    <route id="fiche1_edit" path="/{id}/edit" methods="GET|POST">
        <default key="_controller">CamillePlatformBundle:Fiche1:edit</default>
    </route>

    <route id="fiche1_delete" path="/{id}/delete" methods="DELETE">
        <default key="_controller">CamillePlatformBundle:Fiche1:delete</default>
    </route>

</routes>
